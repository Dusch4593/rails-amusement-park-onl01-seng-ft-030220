<p id="notice"><%= notice %></p>



<% if flash[:message] %>
<div class="center alert">
  <p><%= flash[:message] %></p>
</div>
<% end %>

<h1>Hello, <%= @user.name %>!</h1>

<% if @user.admin %>
  <strong>ADMIN</strong>
<% end %>

<h3>Name <%= @user.name %></h3>
<h3>Mood: <%= @user.mood %></h3>
<h3>Tickets: <%= @user.tickets %></h3>
<h3>Height: <%= @user.height %></h3>
<%= link_to "See attractions", attractions_path %>
